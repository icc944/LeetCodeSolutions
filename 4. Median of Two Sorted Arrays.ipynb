    "class Solution(object):\n",
    "    def findMedianSortedArrays(self, nums1, nums2):\n",
    "        arr = sorted(nums1+nums2)\n",
    "        a, b = len(arr)//2, -((len(arr)//2)+1)\n",
    "        return (arr[a] + arr[b]) /2\n",
    "\n",
    "Solution().findMedianSortedArrays(n1,n2)"
   ]
